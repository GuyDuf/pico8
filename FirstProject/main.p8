function layout(squares)
 	for square in all(squares) do
 		rect(square.x1, square.y1, square.x2, square.y2, square.col)
	end
end
  

function create_plant(x_pos,y_pos,plant_type)
	add(garden, {
		x = x_pos,
		y = y_pos,
		plant_type = plant_type,
		growth = 0,
		sprite = 0,
		draw = function(self)
			spr(self.sprite,self.x*10+16,self.y*10+5)
		end,
		update = function(self)
			if(self.growth <4) then
				self.growth += 1
				self.sprite += 1
			end	
		end
	})		
end


