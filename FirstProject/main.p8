pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
 char = {
		x = 1,
		y = 1,
		sprite = 16,
		cash = 0,
		draw = function(self)
			spr(self.sprite, self.x*10+15, self.y*10+5)
		end,
		update = function(self)
			if(btnp(‚û°Ô∏è) and self.x < 5) then
				self.x += 1
 			else if(btnp(‚¨ÖÔ∏è) and self.x > 1) then
				self.x -= 1
			else if(btnp(‚¨ÜÔ∏è) and self.y > 1) then
				self.y -= 1
			else if(btnp(‚¨áÔ∏è) and self.y < 5) then
				self.y += 1
			end
			end
			end
			end
		end,
		update_cash = function(self, action, amount)
			if(action == "buy") then
				self.cash -= amount
			else if(action == "sell") then
				self.cash += amount
			end
			end
		end
	}
inv = {
	add_plant = function(self, plant)
		add(self, plant)
	end,
	remove_plant = function(self,plant)
		del(self,plant)
	end,
	print = function(self)
		for plant in all(self) do
			print(plant.x)
			print(plant.y)
			print(plant.plant_type)
		end
	end
	}
 cash = 0
 tick = 150
 garden = {
	print = function(self)
			print(count(self))
	end
}
 state = "garden"
 which_one = {x=0, y=0}
 on_top = false
 test = "putain"
end

function _update()
	if(state == "garden") then
		
		if(count(garden)==0) then
			on_top = false
			which_one = {x = 0, y = 0}
		end
		for plant in all(garden) do
			if(char.x == plant.x and char.y == plant.y) then
				on_top = true
				which_one = {x = plant.x, y = plant.y}
				break
			else on_top = false
				 which_one = {x = 0, y = 0}
			end
		end
		
		char:update()
		-- planting --
		
		if(btnp(‚ùé)) then
			if(on_top == false) then
				create_plant(char.x,char.y,1)
			else for plant in all(garden) do
					if(plant.growth > 3 ) then 
						if(char.x == plant.x and char.y == plant.y) then
							inv:add_plant(plant)
			 				del(garden,plant)
						end
					end
				end
			end
		end
		-- growth --  
		tick -= 10
		-- effectu la croissance des plantes a chaque "tick" --
		if(tick < 0) and count(garden) > 0 then
			tick = 150
			for plant in all(garden) do
		 		plant:update()
			end
		end

		if(btnp(üÖæÔ∏è)) then
			test = "fuck"
			state="menu"
		end
		
	end
	if(state == "menu") then
		cursor_pos = 1

		if(cursor_pos == 1) then
			if(btnp(‚ùé) and count(inv)>0) then
				inv:remove_plant(inv[1])
			end
		end	

		--if(btnp(4)) then
	--		close_menu()
	--	end
	end	
end


function _draw()
	cls()
	char:draw()
	inv:print()
	garden:print()
	print(test)
	print(on_top)
	print("position: "..which_one.x.."  "..which_one.y)
	print(direction)
	print("cash : "..cash)
	if(state == "garden") then
		-- for plants in all(garden) do
		--		print(plants)
		--	end
		 print(char.x)
		 print(char.y)
		 -- layout--
			layout(squares)

		 -- draw character --

		for plant in all(garden) do
		 plant:draw()
		end

	else if(state == "menu") then
		
		spr(34,40,40,2,2)
		spr(34,58,40,2,2)
		spr(34,76,40,2,2)
		if(cursor_pos == 1) then
			spr(32,40,40,2,2)
		else if(cursor_pos == 2) then
			spr(32,58,40,2,2)
		else if(cursor_pos == 3) then
			spr(32,76,58,2,2)
		end
		end
		end
	end
	end
end

